{
  "name": "js-dev-env",
  "version": "1.0.0",
  "description": "Javascript Development Environment",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node tools/srcServer.js",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "lint": "esw webpack.config.*, src, tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate-mock-data": "babel-node tools/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonwjudge/js-dev-env.git"
  },
  "author": "Jon Judge",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonwjudge/js-dev-env/issues"
  },
  "dependencies": {
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.24.1",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "cheerio": "0.22.0",
    "compression": "1.6.2",
    "cross-env": "4.0.0",
    "css-loader": "0.28.1",
    "eslint": ">=4.18.2",
    "eslint-plugin-import": "2.2.0",
    "eslint-watch": "3.1.0",
    "express": "4.15.2",
    "extract-text-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "2.28.0",
    "jsdom": "16.5.0",
    "json-schema-faker": "0.4.1",
    "json-server": "0.10.0",
    "localtunnel": "1.8.2",
    "mocha": "3.3.0",
    "nock": "9.0.13",
    "npm-run-all": "4.0.2",
    "nsp": "2.6.3",
    "numeral": "2.0.6",
    "open": ">=6.0.0",
    "rimraf": "2.6.1",
    "style-loader": "0.17.0",
    "webpack": "2.5.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "2.18.0",
    "webpack-md5-hash": "0.0.5"
  }
}
